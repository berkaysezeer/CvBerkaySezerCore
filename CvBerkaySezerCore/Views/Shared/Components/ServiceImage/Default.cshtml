@using EntityLayer.Concrete
@model List<ServiceImage>
<div class="row py-3">
	@{
		int delay = 100;
	}
	@foreach (var image in Model)
	{
		<div class="col-md-3 text-center" data-aos="fade-up" data-aos-delay="@delay">
			<img class="mb-2" src="~/template/images/services/web-design.svg" width="96" height="96" alt="web design" />
			<div class="h5">Web Design</div>
		</div>

		{ delay += 100; }
	}
</div>
</div>